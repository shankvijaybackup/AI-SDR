# RENAMING INSTRUCTION: Rename this file to '.env'

PORT=4000
FRONTEND_ORIGIN=http://localhost:3000

# TWILIO (Required for Calls)
TWILIO_ACCOUNT_SID=replace_with_your_twilio_sid
TWILIO_AUTH_TOKEN=replace_with_your_twilio_token
TWILIO_PHONE_NUMBER=replace_with_your_twilio_number

# NGROK (Required for Twilio Webhooks)
# Run 'ngrok http 4000' and paste the https URL here
PUBLIC_BASE_URL=https://replace-with-your-ngrok-url.ngrok-free.app

# OPENAI (Required for AI responses)
OPENAI_API_KEY=replace_with_your_openai_key

# ELEVENLABS (Optional)
ELEVEN_API_KEY=replace_with_your_elevenlabs_key
ELEVEN_VOICE_ID_MALE=replace_with_voice_id
ELEVEN_VOICE_ID_FEMALE=replace_with_voice_id
